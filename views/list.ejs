<h3>Consultas</h3>
<form action="/searchresult" method="get">
    <input type="search" name="search" id="search" placeholder="Email ou CPF do client">
    <button>Pesquisar</button>
</form>
<hr>
<% appos.forEach(appo => {%>
    <hr>
    <h4>Paciente : <%= appo.name %></h4>
    <h4>Email : <%= appo.email %></h4>
    <h4>CPF : <%= appo.cpf %></h4>
    <hr>
<%})%>